VERSION=0.9

LISP=call-tree.el chart.el dbif-browse.el dbif-edit.el dbif.el \
	eieio-comp.el eieio-doc.el eieio eieio-opt.el eieio.el \
	eieiocomp.el psql.el

TEXINFO=eieio.texi

# dialog code is not distributed.
DIALOG=dialog-mode.el dialog-tree.el dlg-class.el dlg-config.el \
	e-config.el ecfg-menu.el widget-d.el widget-i.el

all:
	emacs -batch -l eieiocomp.el

dist: $(LISP) $(TEXINFO)
	mkdir eieio-$(VERSION)
	cp $(LISP) $(TEXINFO) eieio-$(VERSION)
	tar -cvzf eieio-$(VERSION).tar.gz eieio-$(VERSION)
	rm -rf eieio-$(VERSION)

#end